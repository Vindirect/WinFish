// Insaniquarium.rc

#include <windows.h>
#define FW_NORMAL 400
#define FALSE 0

IDI_MAIN_ICON   ICON   "Insaniquarium.ico"

/////////////////////////////////////////////////////////////////////////////
//
// Game Definition
//

__GDF_XML       DATA    "gdf.xml"

/////////////////////////////////////////////////////////////////////////////
//
// Menu
//

102 MENU
LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
{
    POPUP "Misc"
    {
        MENUITEM "Paste", 106
    }
}

/////////////////////////////////////////////////////////////////////////////
//
// Dialog
//

107 DIALOGEX 0, 0, 186, 44
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "Enter Steam User Name"
LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
FONT 8, "MS Shell Dlg", FW_NORMAL, FALSE, 1
{
    CONTROL "OK", 1, "BUTTON", BS_DEFPUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 129, 7, 50, 14
    CONTROL "Cancel", 2, "BUTTON", BS_PUSHBUTTON | WS_CHILD | WS_VISIBLE | WS_TABSTOP, 129, 24, 50, 14
    CONTROL "", 1001, "EDIT", ES_LEFT | ES_AUTOHSCROLL | WS_CHILD | WS_VISIBLE | WS_BORDER | WS_TABSTOP, 4, 14, 120, 12
    CONTROL "Steam User Name:", -1, "STATIC", SS_LEFT | WS_CHILD | WS_VISIBLE | WS_GROUP, 4, 4, 80, 8
}

/////////////////////////////////////////////////////////////////////////////
//
// Version
//

VS_VERSION_INFO VERSIONINFO
FILEVERSION 1, 1, 0, 0
PRODUCTVERSION 1, 1, 0, 0
FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
FILEFLAGS 0x1L
#else
FILEFLAGS 0x0L
#endif
FILEOS 0x4
FILETYPE 0x1L
FILESUBTYPE 0x0L
BEGIN
BLOCK "StringFileInfo"
BEGIN
BLOCK "040904b0"
BEGIN
VALUE "CompanyName", "PopCap Games"
VALUE "FileDescription", "Insaniquarium"
VALUE "FileVersion", "1.1.0.0"
VALUE "InternalName", "WinFish"
VALUE "LegalCopyright", "Copyright (C) 2004"
VALUE "OriginalFilename", "WinFish.exe"
VALUE "ProductName", "Insaniquarium"
VALUE "ProductVersion", "1.1"
END
END
BLOCK "VarFileInfo"
BEGIN
VALUE "Translation", 0x409, 1200
END
END